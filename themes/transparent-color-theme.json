{
	"name": "transparent",
	"colors": {
		"window.activeBorder": "#000",
		"focusBorder": "#B994FF",
		"editor.background": "#000000",
		"editor.foreground": "#FCCAD2",
	// Side Bar — The Side Bar contains views like the Explorer and Search.
		"sideBar.background": "#000000",
		"sideBar.foreground": "#FCCAD2",
		"sideBar.border": "#B994FF",
		"sideBar.dropBackground": "#000000",
		"sideBarTitle.foreground": "#FCCAD2",
		"sideBarSectionHeader.border": "#B994FF",
		"sideBarSectionHeader.background": "#000",
		"sideBarSectionHeader.foreground": "#FC6FC4",
	// Activity Bar — The Activity Bar is displayed either on the far left or right of the workbench and allows fast switching between views of the Side Bar.
		"activityBar.background": "#000000",
		"activityBar.foreground": "#B994FF",
		"activityBar.border": "#B994FF",
		"activityBarBadge.background": "#000000",
		"activityBarBadge.foreground": "#B994FF",
	// Editor Groups & Tabs — Editor Groups are the containers of editors. There can be up to three editor groups. A Tab is the container of an editor. Multiple Tabs can be opened in one editor group.
		"editorGroup.border": "#B994FF",
		"editorGroup.dropBackground": "#000000",

		"editorGroupHeader.noTabsBackground": "#0F0F0F",
		"editorGroupHeader.tabsBackground": "#000000",
		"editorGroupHeader.tabsBorder": "#B994FF",
		
		"tab.activeBackground": "#000000",
		"tab.activeForeground": "#FC6FC4",
		"tab.border": "#B994FF",
		"tab.activeBorder": "#000000",

		"tab.unfocusedActiveForeground": "#a06ffd",
		"tab.unfocusedActiveBackground": "#000000",
		"tab.unfocusedActiveBorder": "#000000",
		
		"tab.inactiveBackground": "#000",
		"tab.inactiveForeground": "#FCCAD2",
		
		"tab.unfocusedInactiveForeground": "#FCCAD2",
		
		"tab.hoverBackground": "#3C2666",
		"tab.hoverBorder": "#3C2666",
		
		"tab.unfocusedHoverBackground": "#3C2666",
		"tab.unfocusedHoverBorder": "#3C2666",
    // Lists and Trees — Colors for list and trees like the File Explorer. An active list/tree has keyboard focus, an inactive does not.
		"list.activeSelectionBackground": "#000",
		"list.dropBackground": "#FCCAD2",
		"list.focusBackground": "#a06ffd",
		"list.focusForeground": "#FCCAD2",
		"list.highlightForeground": "#FCCAD2",
		"list.hoverBackground": "#a06ffd",
		"list.hoverForeground": "#FCCAD2",
		"list.inactiveSelectionBackground": "#3C2666",
		"list.inactiveSelectionForeground": "#FC6FC4",
		"list.inactiveFocusBackground": "#7A4ECC",
		"list.invalidItemForeground": "#FCCAD2",
	
	// Input Control — Colors for input controls such as in the Search view or the Find/Replace dialog.
		"input.background": "#000000",
		"input.border": "#7A4ECC",
		"input.foreground": "#FCCAD2",
		"inputOption.activeBorder": "#a06ffd",
	// Editor Colors — for syntax highlighting, use the editor.tokenColorCustomizations setting. All other editor colors are listed here
		"editorCursor.background": "#FC6FC4",
		"editorCursor.foreground": "#FC6FC4",
		"editor.selectionBackground": "#3C2666",
		"editorLineNumber.foreground": "#5d3b9b",
		"editorLineNumber.activeForeground": "#FC6FC4",
	// Panel Colors — Panels are shown below the editor area and contain views like Output and Integrated Terminal.
		"panel.background": "#000",
		"panel.border": "#FC6FC4",
		"panelTitle.activeBorder": "#FC6FC4",
		"panelTitle.activeForeground": "#FC6FC4",
		"panelTitle.inactiveForeground": "#a06ffd",

		"statusBar.background": "#a06ffd",
		"button.background": "#000",
		"button.foreground": "#B994FF",
		"button.border": "#B994FF",
		"button.hoverBackground": "#111"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
